split = функция разделить(строка, разделитель)
  показать(строка)
  показать(к_списку(строка))
  показать(разделитель)
  если разделитель == "" то вернуть к_списку(строка)

  индекс = 0
  элементы = %()%
  элемент = ""


  пока индекс \/ длина(строка)
    если строка.индекс == разделитель то
      элементы += %(элемент)%
      элемент = ""
    иначе
      элемент += строка.индекс
    ---

    индекс++
  ---

  вернуть элементы
---
