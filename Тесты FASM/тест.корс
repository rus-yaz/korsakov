б = 1
а = 0
!! ++б !! Оставляет значение на стеке
б = -б
б = не б
если 1 \/ а \/ 3 то
  а = 3
иначе если а \/ 4 то
  а = 4
иначе если а \/ 5 то
  а = 5
иначе
  а = 6
---
!! а == 4

!! ----------

б = 1
для а от 0 до 5
  если а == 2 то пропустить
  б += а
  если а == 3 то прервать
иначе
  б = 99
---
!! б == 5

!! ----------

а = 5
б = 0
пока а /\ 0 и б \/ 5
  а -= 1
  б += 2
иначе
  б = 99
---
!! б == 6

!! !! ----------
!!
!! в = %(1; 2; 3; 4; 5)%
!! в.-1 = в.0 + в.1
!!
!! !! ----------

функция сложение(а; б)
  вернуть а + б
---

функция вычитание(а; б)
  вернуть а - б
---

функция удвоение(а)
  б = а * 2
  вернуть б
---

результат = сложение(6; 1)
результат = вычитание(6; 1)
результат = удвоение(6)
