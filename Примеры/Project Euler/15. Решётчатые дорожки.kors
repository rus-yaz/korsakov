функция решётчатые_дорожки(размерность = нуль)
  если размерность == нуль то размерность = ввести_число("Введите размерность поля: ")
  количество_путей = 1
  для хождение от 1 до размерность + 1: количество_путей *= размерность / хождение + 1
  показать("Количество возможных путей: " + к_строке(к_целому(количество_путей)))
  вернуть к_целому(количество_путей)
