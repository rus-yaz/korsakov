а = 10
показать(а)

пока а
  а -= 1
---
показать(а)

для б от 0 до 5
  а += б
---
показать(а)

для б от 0 до 5 через 2
  а += б
---
показать(а)

для а от 0 до 10
  для б от 1 до 3
  ---
  если а == 5 то
    прервать
  ---
---
показать(а б)

а = 0
б = 2
показать(а б)

если а /\ 1 то
  а = 1
иначе если а == 0 то
  а = 1
иначе если б то
  б = 0
иначе
  а = 0
---
показать(а б)

а = %(
  %(0 0 0)
  %(0 0 0)
  %(0 0 0)
)
показать(а)

а = "Привет, мир!"
показать(а)

а.6 = "!"
а.8 = "М"

показать(а)

показать("Привет,\н\тмир!")

показать("2 * 2 = \(2 * 2)")

показать(разделить("Привет, мир!"))

ошибка("Текст ошибки")

а = 3
пока а--
  установить_семя(123)
  показать()

  показать(получить_псевдослучайное_число(0 100))
  показать(получить_псевдослучайное_число(0 100))

  показать(получить_случайное_число(0 100))
---

показать()

а = 3,2
б = 2,3

показать(а + б)

функция а(_1 _2=0)
  _1 += _2
  123
  вернуть _1
---

функция б(_1 _2=0)
  _1 += _2
  123
  вернуть
---

функция в(_1 _2=0)
  _1 += _2
  123
---

функция г()
  если 1 то
    123
    вернуть 1
  ---
---

показать(а(1))
показать(а(1 2))

показать()

показать(б(1))
показать(б(1 2))

показать()

показать(в(1))
показать(в(1 2))

показать()

показать(г())
показать(г())
