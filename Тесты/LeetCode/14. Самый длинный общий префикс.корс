функция найти_общий_префикс(строки)
  префикс = ""
  выход = 0
  для символ из разделить(строки.0)
    для строка из строки
      если выход то прервать
      выход = найти_индекс(строка; префикс + символ) == -1
    ---
    если выход то прервать
    префикс += символ
  ---
  вернуть префикс
---

показать(найти_общий_префикс(%("flower"; "flow"; "flight")%))
показать(найти_общий_префикс(%("dog"; "racecar"; "car")%))
