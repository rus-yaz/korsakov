функция а(б в=0)
  б += в
---

а(1)
а(1 2)

а = 10

пока а
  а -= 1
---

для б от 0 до 5
  а += б
---

для б от 0 до 5 через 2
  а += б
---

для а от 0 до 10
  для б от 1 до 3
  ---
  если а == 5 то
    прервать
  ---
---

а = 0
б = 2

если а /\ 1 то
  а = 1
иначе если а == 0 то
  а = 1
иначе если б то
  б = 0
иначе
  а = 0
---

а = %(
  %(0 0 0)
  %(0 0 0)
  %(0 0 0)
)
а = "Привет, мир!"
а.6 = "!"
а.8 = "М"
