!!функция а(б в=0)
!!  б += в
!!---
!!
!!а(1)
!!а(1 2)

показать(разделить("Привет, мир!"))

а = 10
показать(а)

пока а
  а -= 1
---
показать(а)

для б от 0 до 5
  а += б
---
показать(а)

для б от 0 до 5 через 2
  а += б
---
показать(а)

для а от 0 до 10
  для б от 1 до 3
  ---
  если а == 5 то
    прервать
  ---
---
показать(а б)

а = 0
б = 2
показать(а б)

если а /\ 1 то
  а = 1
иначе если а == 0 то
  а = 1
иначе если б то
  б = 0
иначе
  а = 0
---
показать(а б)

а = %(
  %(0 0 0)
  %(0 0 0)
  %(0 0 0)
)
показать(а)

а = "Привет, мир!"
показать(а)

а.6 = "!"
а.8 = "М"

показать(а)
